# Kafka Configuration
KAFKA_ENABLED=true
KAFKA_BROKERS=kafka:9092
KAFKA_SSL=false
KAFKA_SASL_MECHANISM=
KAFKA_SASL_USERNAME=
KAFKA_SASL_PASSWORD=

# Kafka Connection Settings
KAFKA_CONNECTION_TIMEOUT=10000
KAFKA_REQUEST_TIMEOUT=30000
KAFKA_RETRY_INITIAL_TIME=100
KAFKA_RETRY_COUNT=8

# Kafka Producer Settings
KAFKA_PRODUCER_TRANSACTION_TIMEOUT=60000
KAFKA_PRODUCER_MAX_IN_FLIGHT=5
KAFKA_PRODUCER_IDEMPOTENT=true

# Kafka Consumer Settings
KAFKA_CONSUMER_GROUP_ID=aikb-consumer-group
KAFKA_CONSUMER_SESSION_TIMEOUT=30000
KAFKA_CONSUMER_HEARTBEAT_INTERVAL=3000
KAFKA_CONSUMER_MAX_WAIT_TIME=5000
KAFKA_CONSUMER_AUTO_TOPIC_CREATION=true
KAFKA_CONSUMER_AUTO_OFFSET_RESET=latest

# Kafka Topic Configuration
KAFKA_TOPIC_ENTITY_EVENTS_PARTITIONS=3
KAFKA_TOPIC_ENTITY_EVENTS_REPLICATION_FACTOR=1
KAFKA_TOPIC_KNOWLEDGE_EVENTS_PARTITIONS=3
KAFKA_TOPIC_KNOWLEDGE_EVENTS_REPLICATION_FACTOR=1
KAFKA_TOPIC_ENTITY_VECTOR_PROCESSING_PARTITIONS=6
KAFKA_TOPIC_ENTITY_VECTOR_PROCESSING_REPLICATION_FACTOR=1
KAFKA_TOPIC_KNOWLEDGE_VECTOR_PROCESSING_PARTITIONS=6
KAFKA_TOPIC_KNOWLEDGE_VECTOR_PROCESSING_REPLICATION_FACTOR=1
KAFKA_TOPIC_ENTITY_RELATION_PROCESSING_PARTITIONS=3
KAFKA_TOPIC_ENTITY_RELATION_PROCESSING_REPLICATION_FACTOR=1
KAFKA_TOPIC_DEAD_LETTER_QUEUE_PARTITIONS=3
KAFKA_TOPIC_DEAD_LETTER_QUEUE_REPLICATION_FACTOR=1

# Kafka Health Check
KAFKA_HEALTH_CHECK_TIMEOUT=5000
KAFKA_HEALTH_CHECK_INTERVAL=30000
KAFKA_HEALTH_CHECK_RETRIES=3

# Kafka Monitoring
KAFKA_MONITORING_ENABLED=false
KAFKA_MONITORING_METRICS_INTERVAL=60000
KAFKA_MONITORING_LOG_LEVEL=info

# RabbitMQ Configuration
RABBITMQ_URL=amqp://rabbitmq:5672
RABBITMQ_HOSTNAME=rabbitmq
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=admin
RABBITMQ_PASSWORD=admin123
RABBITMQ_VHOST=my_vhost

# RabbitMQ Connection Settings
RABBITMQ_FRAME_MAX=0
RABBITMQ_HEARTBEAT=60
RABBITMQ_LOCALE=en_US

# RabbitMQ Retry Configuration
RABBITMQ_RETRY_INITIAL_TIME=1000
RABBITMQ_RETRY_COUNT=5
RABBITMQ_RETRY_MAX_TIME=60000

# RabbitMQ Health Check
RABBITMQ_HEALTH_CHECK_TIMEOUT=5000
RABBITMQ_HEALTH_CHECK_INTERVAL=30000
RABBITMQ_HEALTH_CHECK_RETRIES=3

# RabbitMQ Monitoring
RABBITMQ_MONITORING_ENABLED=false
RABBITMQ_MONITORING_METRICS_INTERVAL=60000
RABBITMQ_MONITORING_LOG_LEVEL=info

# PDF Processing Configuration
PDF_SPLIT_THRESHOLD=50
PDF_SPLIT_SIZE=25
PDF_MAX_SPLIT_SIZE=100
PDF_MIN_SPLIT_SIZE=10
PDF_CONCURRENT_PART_PROCESSING=3

# PDF Worker Configuration
WORKER_ID=pdf-splitting-worker-1
MAX_RETRIES=3
TEMP_DIR=/tmp/pdf-processing

# Elasticsearch Configuration
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=changeme
ELASTICSEARCH_API_KEY=
ELASTICSEARCH_VERIFY_SSL=true

# Elasticsearch Logging Configuration
ELASTICSEARCH_LOGGING_ENABLED=false
ELASTICSEARCH_LOG_LEVEL=info
ELASTICSEARCH_LOG_INDEX=logs
ELASTICSEARCH_LOG_INDEX_PATTERN=logs-YYYY.MM.DD