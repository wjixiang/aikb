  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"knowledgeBase/**/*.ts\" \"test/**/*.ts\" \"**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:workers": "pm2 start",
    "stop:workers": "node knowledgeBase/lib/rabbitmq/stop-workers.js",
    "check:workers": "node knowledgeBase/lib/rabbitmq/simple-check-workers.js",
    "check:workers:full": "node knowledgeBase/lib/rabbitmq/check-workers-status.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "export SYSTEM_LOG_LEVEL=3 && vitest --silent --exclude **/*.integration.test.ts --exclude **/*.integrated.test.ts --exclude **/*.interogated.test.ts ",
    "test:integrated": "vitest run knowledgeBase/**/*.integrated.test.ts",
    "test": "vitest run",
    "test:markdown-part-storage": "vitest run knowledgeBase/lib/rabbitmq/__tests__/markdown-part-storage.integration.test.ts",
    "test:markdown-part-storage:unit": "vitest run knowledgeBase/lib/rabbitmq/__tests__/markdown-part-cache-mongodb.test.ts",
    "test:markdown-part-storage:all": "chmod +x knowledgeBase/lib/rabbitmq/__tests__/run-markdown-part-storage-tests.sh && knowledgeBase/lib/rabbitmq/__tests__/run-markdown-part-storage-tests.sh",
    "demo:markdown-part-storage": "npx tsx knowledgeBase/lib/rabbitmq/demo-markdown-part-storage.ts",
    "coverage": "vitest run --coverage",
    "baml:generate": "pnpm exec baml-cli generate",
    "doc": "npx typedoc --entryPointStrategy Expand ./*",
    "cli": "node knowledgeBase/cli/simple-cli.js",
    "search": "node knowledgeBase/cli/interactive-search.js",
    "clear:library:indices": "knowledgeBase/scripts/clear-library-indices.sh"
  },
  "dependencies": {
