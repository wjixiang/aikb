{
  "name": "entity-db",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/knowledgeBase/knowledge-db/entity-db/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project entity-db --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/knowledgeBase/knowledge-db/entity-db",
        "main": "libs/knowledgeBase/knowledge-db/entity-db/src/index.ts",
        "tsConfig": "libs/knowledgeBase/knowledge-db/entity-db/tsconfig.lib.json",
        "assets": ["libs/knowledgeBase/knowledge-db/entity-db/*.md"]
      },
      "dependsOn": ["gen-client"]
    },
    "db-pull": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db pull --schema ./src/prisma/schema.prisma",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "db-push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db push --schema ./src/prisma/schema.prisma",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "gen-client": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "reset-db": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate reset --schema ./src/prisma/schema.prisma",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "migrate-addvector": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --name add-pgvector --create-only",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma studio",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate deploy",
        "cwd": "libs/knowledgeBase/knowledge-db/entity-db"
      }
    }
  }
}
