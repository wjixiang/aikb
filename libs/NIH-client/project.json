{
  "name": "NIH-client",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/NIH-client/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project meSH-client --web",
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "libs/NIH-client/jest.config.e2e.js",
        "passWithNoTests": true
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/libs/NIH-client"
      }
    }
  }
}
