{
  "name": "agent-db",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/agent-db/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project agent-db --web",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/agent-db",
        "main": "libs/agent-db/src/index.ts",
        "tsConfig": "libs/agent-db/tsconfig.lib.json",
        "assets": ["libs/agent-db/*.md"]
      },
      "dependsOn": ["gen-client"]
    },
    "db-pull": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db pull --schema ./src/prisma/schema.prisma",
        "cwd": "libs/agent-db"
      }
    },
    "db-push": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma db push --schema ./src/prisma/schema.prisma",
        "cwd": "libs/agent-db"
      }
    },
    "gen-client": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma generate --schema ./src/prisma/schema.prisma",
        "cwd": "libs/agent-db"
      }
    },
    "reset-db": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate reset --schema ./src/prisma/schema.prisma",
        "cwd": "libs/agent-db"
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate dev --schema ./src/prisma/schema.prisma",
        "cwd": "libs/agent-db"
      }
    },
    "studio": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma studio",
        "cwd": "libs/agent-db"
      }
    },
    "deploy": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npx prisma migrate deploy",
        "cwd": "libs/agent-db"
      }
    }
  }
}
