{
  "name": "bibliography",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/bibliography/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/bibliography",
        "main": "libs/bibliography/src/index.ts",
        "tsConfig": "libs/bibliography/tsconfig.lib.json",
        "assets": ["libs/bibliography/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vite:test"
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/bibliography/**/*.ts"]
      }
    }
  }
}
