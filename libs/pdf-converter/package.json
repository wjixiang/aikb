{
  "name": "@aikb/pdf-converter",
  "version": "1.0.0",
  "description": "PDF converter library with MinerU integration and image upload capabilities",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "package": "pnpm run build && pnpm pack",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "pdf",
    "converter",
    "mineru",
    "markdown",
    "ocr",
    "document-conversion",
    "image-upload"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "yauzl": "^3.2.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/yauzl": "^2.10.3",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "mineru-client": "workspace:*",
    "@aikb/s3-service": "workspace:*",
    "log-management": "workspace:*",
    "dotenv": "^17.2.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  }
}