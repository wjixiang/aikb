{
  "name": "llm-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/llm-api/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/llm-api",
        "main": "libs/llm-api/src/index.ts",
        "tsConfig": "libs/llm-api/tsconfig.lib.json",
        "assets": ["libs/llm-api/*.md"]
      }
    },
    "test": {
      "executor": "@nx/vitest:test",
      "outputs": ["{options.reportsDirectory}"],
      "options": {
        "reportsDirectory": "../../coverage/libs/llm-api"
      }
    }
  }
}
