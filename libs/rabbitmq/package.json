{
  "name": "@aikb/rabbitmq",
  "version": "1.0.0",
  "description": "RabbitMQ messaging library with PDF processing workers support",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "package": "pnpm run build && pnpm pack",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "rabbitmq",
    "amqp",
    "stomp",
    "messaging",
    "pdf-processing",
    "workers",
    "queue",
    "message-broker"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "amqplib": "^0.10.4",
    "@stomp/stompjs": "^7.2.1",
    "ws": "^8.18.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/amqplib": "^0.10.7",
    "@types/node": "^22.10.7",
    "@types/uuid": "^9.0.8",
    "@types/ws": "^8.18.1",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "log-management": "workspace:*",
    "@aikb/pdf-converter": "workspace:*",
    "@aikb/s3-service": "workspace:*",
    "mineru-client": "workspace:*",
    "@aikb/embedding": "workspace:*",
    "@aikb/chunking": "workspace:*",
    "dotenv": "^17.2.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  }
}