{
  "name": "@aikb/s3-service",
  "version": "1.0.0",
  "description": "Independent S3 service module for file upload, download, and management",
  "main": "./src/index.js",
  "types": "./src/index.d.ts",
  "type": "commonjs",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "s3",
    "aws",
    "aliyun",
    "oss",
    "file-upload",
    "file-storage"
  ],
  "author": "",
  "license": "UNLICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.901.0",
    "@aws-sdk/s3-request-presigner": "^3.901.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "dotenv": "^17.2.2"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "require": "./src/index.js"
    },
    "./mock": {
      "types": "./src/mock.d.ts",
      "import": "./src/mock.js",
      "require": "./src/mock.js"
    }
  }
}