{
  "name": "bibliography-service-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["bibliography-service"],
  "targets": {
    "e2e": {
      "executor": "@nx/vite:test",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "dependsOn": ["bibliography-service:build","bibliography-service:serve"]
    },
    "testUploadPdf": {
      "command": "npx tsx '/workspace/scripts/upload-pdf.ts' '/workspace/test/ACEI.pdf'",
      "dependsOn": [
        "bibliography-service:serve",
        "pdf2md-service:serve"
      ]
    }
  }
}
