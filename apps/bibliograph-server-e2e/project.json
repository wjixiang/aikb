{
  "name": "bibliograph-server-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "implicitDependencies": ["bibliograph-server"],
  "targets": {
    "e2e": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{e2eProjectRoot}"],
      "options": {
        "jestConfig": "apps/bibliograph-server-e2e/jest.config.ts",
        "passWithNoTests": true
      },
      "dependsOn": ["bibliograph-server:build", "bibliograph-server:serve"]
    }
  }
}
