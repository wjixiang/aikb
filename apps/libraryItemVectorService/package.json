{
  "name": "library-item-vector-service",
  "version": "0.0.1",
  "description": "Library item vector service for semantic search and embedding management",
  "main": "dist/main.js",
  "scripts": {
    "build": "webpack-cli build --node-env=production",
    "build:dev": "webpack-cli build --node-env=development",
    "start": "node dist/main.js",
    "start:dev": "webpack-cli serve --node-env=development",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run --config vitest.config.ts --testNamePattern=e2e"
  },
  "dependencies": {
    "embedding": "workspace:*",
    "item-vector-storage": "workspace:*",
    "proto-ts": "workspace:*",
    "@grpc/grpc-js": "^1.13.3",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^11.0.0",
    "@nestjs/microservices": "^11.1.8",
    "@nestjs/platform-express": "^11.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "google-protobuf": "^4.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@grpc/proto-loader": "^0.8.0",
    "@nestjs/testing": "^11.0.1",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "grpc-js": "link:@types/@grpc/grpc-js",
    "grpc-tools": "^1.13.0",
    "protoc-gen-ts": "^0.8.7",
    "supertest": "^7.0.0",
    "ts-proto": "^2.8.3",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4",
    "webpack-cli": "^5.1.4"
  }
}