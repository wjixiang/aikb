# RabbitMQ Configuration
RABBITMQ_HOST=localhost
RABBITMQ_PORT=5672
RABBITMQ_USERNAME=guest
RABBITMQ_PASSWORD=guest
RABBITMQ_VHOST=/

# S3 Configuration
S3_BUCKET=pdf-processing-bucket
S3_REGION=us-east-1
S3_ACCESS_KEY=your-access-key
S3_SECRET_KEY=your-secret-key
S3_ENDPOINT=

# PDF Processing Configuration
PDF_SPLIT_SIZE=25
PDF_MAX_SPLIT_SIZE=100
PDF_MIN_SPLIT_SIZE=10
PDF_CONCURRENT_PART_PROCESSING=3

# Worker Configuration
WORKER_ID=pdf-splitting-worker-1
LOG_LEVEL=INFO
MAX_RETRIES=3
TEMP_DIR=/tmp/pdf-processing

# Elasticsearch Logging Configuration
ELASTICSEARCH_LOGGING_ENABLED=false
ELASTICSEARCH_LOG_LEVEL=info
ELASTICSEARCH_URL=http://localhost:9200
ELASTICSEARCH_USERNAME=elastic
ELASTICSEARCH_PASSWORD=changeme
ELASTICSEARCH_API_KEY=
ELASTICSEARCH_VERIFY_SSL=true
ELASTICSEARCH_LOG_INDEX=logs
ELASTICSEARCH_LOG_INDEX_PATTERN=logs-YYYY.MM.DD
SERVICE_NAME=pdf-splitting-worker
NODE_ENV=development

# Enhanced Logging Configuration
DEBUG_MODE=false
DEBUG_LOG_LEVEL=DEBUG
INCLUDE_CONTEXT_IN_CONSOLE=true
PERFORMANCE_TRACKING=true
LOG_PERFORMANCE_THRESHOLD=1000.0